/* ▼▼▼ メインリストのテーブル関係 ▼▼▼ */
#clone-table tbody td {
  border: 0;}

/* テーブルセパレート設定 */
table.table.list-table tr > th.table-separate,
table.table.list-table tr > td.table-separate {
  padding: .75rem 0;
  border: 0;
  width: 6px;
  background-color: #f2f2f2;}
  table.table.list-table tr > th.table-separate .connection,
  table.table.list-table tr > td.table-separate .connection {
