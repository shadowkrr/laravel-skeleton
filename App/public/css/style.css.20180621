/* ▼▼▼ パネル関係 ▼▼▼ */
.card {
  width: 100%;
  box-shadow: 0 2px 30px -2px #b3b3b3;}
  .card a {
    color: #b3b3b3;}
  .card .card-header {
    position: relative;
    background-color: #fff;
    text-align: center;
    letter-spacing: .25rem;
    border-bottom: 0;}
    .card .card-header h5 {
      margin-bottom: .25rem;
      line-height: 1;}
    .card .card-header h6 {
      margin-bottom: 0;
      line-height: 1;
      letter-spacing: 0;
      font-weight: normal;}
    .card .card-header .back-link {
      position: absolute;
      top: 1.5rem;
      left: 1.25rem;
      letter-spacing: 0.125rem;}
    .card .card-header .show-user-info {
      position: absolute;
      top: 1.5rem;
      right: 1.25rem;
      letter-spacing: 0.125rem;}
      .card .card-header .show-user-info i {
        margin: 0 .25rem;}
  .card .card-body {
    position: relative;
    border-top: 1px solid rgba(0,0,0,.125);}
    .card .card-body .form-group {
      margin-top: -.75rem;
      margin-bottom: 1.75rem;}
      .card .card-body .form-group label {
        font-size: .8rem;
        font-weight: bold;
        letter-spacing: .8px;
        margin-bottom: 5px;}
      .card .card-body .form-group .form-control-plaintext {
        line-height: 1;
        padding: 3px 5px;
        margin-bottom: 0;
        border-radius: 6px;
        color: #333333;
        background-color: #e6e6e6;}
        .card .card-body .form-group .form-control-plaintext button {
          padding: .25rem;;
          border: 0;
          background-color: transparent;
          margin: -.25rem 0 -.25rem .25rem;
          color: #b3b3b3;}
    .card .card-body .mail-address:last-child {
      margin-bottom: -1.25rem;}
    .card .card-body .mail-address .form-group label {
      display: inline;
      margin-right: .5rem;}
    .card .card-body .mail-address .form-group .form-control-plaintext {
      width: auto;
      display: inline-block;}
    .card .card-body > .row .check-group {
      position: relative;
      font-size: .8rem;}
      .card .card-body > .row .check-group .check-wrap {
        position: absolute;
        bottom: 0;
        right: 0;
        text-align: right;}
        .card .card-body > .row .check-group .check-wrap .checkbox-inline .checkbox-label {
          margin-right: .5rem;}
  .card.card-check-result .card-body.result-body{
    padding: 0;}
  .card.card-check-result .card-body.result-body:last-child{
    border-bottom: 1px solid rgba(0,0,0,.125);}
    .card.card-check-result .card-body.result-body .check-title {
      padding: 1.25rem;
      padding-right: 0;
      padding-left: calc(1.25rem + 15px);}
    .card.card-check-result .card-body.result-body .check-result {
      color: #333333;}
      .card.card-check-result .card-body.result-body .check-result > div {
        padding: 1.25rem 0;}
        .card.card-check-result .card-body.result-body .check-result > div i {
          padding: 0 1.25rem;}
          .card.card-check-result .card-body.result-body .check-result > div i.fa-times {
            font-size: 22px;
            margin: 0 1px;}
          .card.card-check-result .card-body.result-body .check-result > div i.fa-circle {
            font-size: 18px;}
      .card.card-check-result .card-body.result-body .check-result > div:not(:last-child) {
        border-bottom: 1px solid rgba(0,0,0,.125);}
/* ▲▲▲ パネル関係 ▲▲▲ */

/* ▼▼▼ フォーム関係 ▼▼▼ */
.checkbox input[type=checkbox],
.checkbox-inline input[type=checkbox],
.radio input[type=radio], .radio-inline input[type=radio]{
  position: absolute;
  opacity: 0;
  visibility: hidden;
}
label.checkbox,
label.radio {
  cursor: pointer;
  display: block;
  margin-bottom: .25rem;}
  .checkbox .checkbox-parts,
  .radio .radio-parts{
    cursor: pointer;
    display: inline-block;}

.checkbox-inline,
.radio-inline {
  cursor: pointer;
  display: inline-block;
  padding-left: 0;}
  .checkbox-inline .checkbox-label,
  .radio-inline .radio-label {
    display: inline-block;}
  .checkbox-inline .checkbox-parts,
  .radio-inline .radio-parts{
    cursor: pointer;
    display: inline-block;
    padding-left: 20px;
    position:relative;
    margin-right: 20px;}

input[type=radio]:disabled + label,
input[type=checkbox]:disabled + .checkbox-parts{
  cursor: not-allowed;}
  input[type=radio]:disabled + label::before,
  input[type=checkbox]:disabled + .checkbox-parts::before{
    background-color: #eee;
    cursor: not-allowed;}

input[type=radio] + .radio-parts{
  padding-left: 24px;
  position:relative;
  margin-right: 20px;
  font-weight: normal;}
  .radio input[type=radio] + .radio-parts{
    padding-left: 36px;}
  input[type=radio] + .radio-parts::before{
    content: "";
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 20px;
    height: 20px;
    border: 2px solid #b3b3b3;
    background-color: #fff;
    border-radius: 50%;}
  input[type=radio]:checked + .radio-parts::after{
    content: "";
    display: block;
    position: absolute;
    top: 6px;
    left: 6px;
    width: 8px;
    height: 8px;
    background: #f02475;
    border-radius: 50%;}

input[type=checkbox] + .checkbox-parts{
  padding-left: 24px;
  position:relative;
  margin-right: 0;
  display: inline-block;
  font-weight: normal;}
  input[type=checkbox] + .checkbox-parts::before{
    content: "";
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 18px;
    height: 18px;
    border: 3px solid #b3b3b3;}
  input[type=checkbox]:checked + .checkbox-parts::after {
    content: "";
    display: block;
    position: absolute;
    top: 1px;
    left: 4px;
    width: 9px;
    height: 12px;
    transform: rotate(40deg);
    border-bottom: 3px solid #f02475;
    border-right: 3px solid #f02475;}

/* チェックボックスの位置調整 */
.checkbox-label + input[type=checkbox] + .checkbox-parts::before {
  top: -10px;
  left: 3px;}
.checkbox-label + input[type=checkbox]:checked + .checkbox-parts::after {
  top: -9px;
  left: 7px;}
/* インライン時 */
.checkbox-inline .checkbox-label + input[type=checkbox] + .checkbox-parts::before {
  top: -14px;}
.checkbox-inline .checkbox-label + input[type=checkbox]:checked + .checkbox-parts::after {
  top: -13px;}

label.file-import {
  display: inline-block;
  overflow: hidden;
  position: relative;
  padding: .5em 5rem;}
  label.file-import input[type="file"]{
    opacity: 0;
    padding: 0;
    position: absolute;
    top: 0;
    right: 0;
    margin: 0;
    font-size: 100px;
    cursor: pointer;}
/* ▲▲▲ フォーム関係 ▲▲▲ */

/* ▼▼▼ ログインフォーム関係 ▼▼▼ */
.login-form {
  width: 300px;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;}
  .login-form .input-group .input-group-text {
    font-size: 1.25rem;
    border-width: 2px;
    border-right: 0;
    border-color: #23272b;
    background-color: #ffffff;}
  .login-form .input-group .form-control {
    padding: .75em;
    padding-left: 0;
    border-width: 2px;
    border-left: 0;
    border-color: #23272b;}
  .login-form .btn {
    padding: 13px;}
/* ▲▲▲ ログインフォーム関係 ▲▲▲ */

/* ▼▼▼ モーダル関係 ▼▼▼ */
.modal-dialog {
  max-width: 640px;}
  .modal-dialog .modal-content {
    border-radius: 6px;
    border-top-left-radius: 12px 6px;
    border-top-right-radius: 12px 6px;
    overflow: hidden;}
  .modal-dialog .modal-content .modal-header {
    margin: 0 2rem;
    color: #808080;}
    .modal-dialog .modal-content .modal-header .modal-title{
      width: 100%;
      text-align: center;}
    .modal-dialog .modal-content .modal-header .close {
      position: absolute;
      right: 3rem;
      font-size: 2rem;}
    .modal-dialog .modal-content .modal-header .close:hover{
      color: #808080;
      opacity: 1;}
  .modal-dialog .modal-content .modal-body {
    margin: 0 2rem;}
  .modal-dialog .modal-content .modal-footer {
    background-color: #f7f7f7;
    border-top: 0;
    padding: .75rem;
    justify-content: center;}
    .modal-dialog .modal-content .modal-footer .btn {
      font-size: 1.2rem;
      padding: .5rem 4rem;}
/* ▲▲▲ モーダル関係 ▲▲▲ */
