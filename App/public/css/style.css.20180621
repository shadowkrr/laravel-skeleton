/* ▼▼▼ ヘッダナビ関係 ▼▼▼ */
.navbar {
  padding-top: 1.1rem;
  padding-bottom: 1.1rem;
  position: relative;
}
.navbar-border::after,
.modal-dialog .modal-content::before {
  content: '';
  display: block;
  height: 7px;
  width: 100%;

  background: #479ea5; /* Old browsers */
  background: -moz-linear-gradient(left, #479ea5 0%, #b6cba4 50%, #e6db9a 66%, #d28b5f 100%); /* FF3.6-15 */
  background: -webkit-linear-gradient(left, #479ea5 0%,#b6cba4 50%,#e6db9a 66%,#d28b5f 100%); /* Chrome10-25,Safari5.1-6 */
  background: linear-gradient(to right, #479ea5 0%,#b6cba4 50%,#e6db9a 66%,#d28b5f 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#479ea5', endColorstr='#d28b5f',GradientType=1 ); /* IE6-9 */
}
header a.color-dark {
  color: #2f2a2b;
}
.navbar-expand>.container,
.navbar-expand>.container-fluid {
  padding-left: 15px;
  padding-right: 15px;
}
.navbar-brand {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 2.5rem;
  font-weight: bold;}

.dropdown-menu.user-menu {
  left: auto;
  right: 0;}
.dropdown-item {
  padding: .5rem 2.5rem .5rem 1.5rem;}
.nav-link i,
.dropdown-item i {
  margin-right: .5em;}
  .dropdown-item i.fa-clipboard-list{
    padding-left: 2px;
    padding-right: 2px;}
  .dropdown-item i.deg-90 {
    transform: rotate(90deg);}

/* caret */
.dropdown-toggle::after {
  width: 13px;
  height: 13px;
  margin-left: 1.5em;
  border: solid;
  border-color: initial;
  border-width: 0 2px 2px 0;
  transform: rotate(45deg);}

/* 絞り込みドロップダウン */
.navbar .refine-dropdown{
  position: absolute;
  color: #fff;
  top: 2.2rem;
  margin-left: -15px;
  border-top-left-radius: 6px;
  border-top-right-radius: 6px;}
  .navbar .refine-dropdown a {
    color: #fff;}
    .navbar .refine-dropdown a.nav-link {
      width: 172px;
      padding-left: 1.5rem;}
      .navbar .refine-dropdown a.nav-link i {
        float: right;
        padding-top: .25rem;}
  .navbar .refine-dropdown .dropdown-toggle::after {
    content: none;}
  .navbar .refine-dropdown .dropdown-menu {
    width: 512px;
    color: #fff;
    border: 0;
    border-radius: 6px;
    border-top-left-radius: 0;
    margin-top: 7px;}
    .navbar .refine-dropdown .dropdown-menu .dropdown-item {
      font-size: .9rem;
      padding: .25rem 0;
      background-color: inherit;}
      .navbar .refine-dropdown .dropdown-menu .dropdown-item:hover {
        text-decoration: underline;}
  .navbar .refine-dropdown .form-control {
    font-size: .8rem;}

/* 絞り込みキャレットの切り替え */
.navbar .refine-dropdown .dropdown .fa-caret-down{
  display: block;}
  .navbar .refine-dropdown .dropdown.show .fa-caret-down{
    display: none;}
.navbar .refine-dropdown .dropdown .fa-times{
  display: none;}
  .navbar .refine-dropdown .dropdown.show .fa-times{
    display: block;}

/* 絞り込みドロップダウン背景色 */
.navbar .refine-dropdown.bg-dark{
  background-color: #2f2a2b;}
  .navbar .refine-dropdown.bg-dark .dropdown.show{
    border-bottom: 8px solid #2f2a2b;}
    .navbar .refine-dropdown.bg-dark .dropdown .dropdown-menu {
      background-color: #2f2a2b;}
      .navbar .refine-dropdown.bg-dark .dropdown .dropdown-menu .btn {
        color: #2f2a2b;
        background-color: #fff;}
      .navbar .refine-dropdown.bg-dark .dropdown .dropdown-menu input[type=radio] + .radio-parts::before {
        border: 0;}
      .navbar .refine-dropdown.bg-dark .dropdown .dropdown-menu input[type=radio] + .radio-parts::after {
        background-color: #2f2a2b;}
.navbar .refine-dropdown.bg-warning{
  background-color: #edc343;}
  .navbar .refine-dropdown.bg-warning .dropdown.show{
    border-bottom: 7px solid #edc343;}
    .navbar .refine-dropdown.bg-warning .dropdown .dropdown-menu {
      background-color: #edc343;}
      .navbar .refine-dropdown.bg-warning .dropdown .dropdown-menu .btn {
        color: #edc343;
        background-color: #fff;}
      .navbar .refine-dropdown.bg-warning .dropdown .dropdown-menu input[type=radio] + .radio-parts::before {
        border: 0;}
      .navbar .refine-dropdown.bg-warning .dropdown .dropdown-menu input[type=radio] + .radio-parts::after {
        background-color: #edc343;}
.navbar .refine-dropdown.bg-success{
  background-color: #59bea0;}
  .navbar .refine-dropdown.bg-success .dropdown.show{
    border-bottom: 7px solid #59bea0;}
    .navbar .refine-dropdown.bg-success .dropdown .dropdown-menu {
      background-color: #59bea0;}
      .navbar .refine-dropdown.bg-success .dropdown .dropdown-menu .btn {
        color: #59bea0;
        background-color: #fff;}
      .navbar .refine-dropdown.bg-success .dropdown .dropdown-menu input[type=radio] + .radio-parts::before {
        border: 0;}
      .navbar .refine-dropdown.bg-success .dropdown .dropdown-menu input[type=radio] + .radio-parts::after {
        background-color: #59bea0;}
