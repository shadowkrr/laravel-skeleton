    table.table.list-table tr > th.table-separate .connection.connection-dark-green,
    table.table.list-table tr > td.table-separate .connection.connection-dark-green{
      background-color: #60a8a6;}
    table.table.list-table tr > th.table-separate .connection.connection-yellow-green,
    table.table.list-table tr > td.table-separate .connection.connection-yellow-green{
      background-color: #c1dba9;}
    table.table.list-table tr > th.table-separate .connection.connection-yellow,
    table.table.list-table tr > td.table-separate .connection.connection-yellow{
      background-color: #e6db9a;}
    table.table.list-table tr > th.table-separate .connection.connection-orange,
    table.table.list-table tr > td.table-separate .connection.connection-orange{
      background-color: #dbaf78;}
  /* 管理画面一覧の場合はpaddingを大きめに */
  table.table.list-table.main-list-table-admin tr > th.table-separate,
  table.table.list-table.main-list-table-admin tr > td.table-separate {
    padding: 1.5rem 0;}
/* テーブルオフセット設定 */
table.table.list-table tbody tr > th.offset,
table.table.list-table tbody tr > td.offset,
table.table.list-table tbody tr > th.offset:first-child
table.table.list-table tbody tr > td.offset:first-child
table.table.list-table tbody tr > th.offset:last-child,
table.table.list-table tbody tr > td.offset:last-child {
  padding: 0;
  border: 0 !important;
  background-color: transparent;}

/* テーブル基本設定 */
table.table.list-table {
  border-collapse: separate;
  border-spacing: 0 .7rem;
  table-layout: fixed;
  word-break: break-all;
  height: 100%;}
  table.table.list-table tbody > tr > td {
    color: #808080;
    background-color: #fff;
    border: 1px solid #dee2e6;
    border-left: 0;
    border-right: 0;
    height: 100%;
    padding: .65rem .65rem 0 .65rem;}
    table.table.list-table tbody > tr > td:first-child,
    table.table.list-table tbody > tr > td.cluster-first {
      border-left: 1px solid #dee2e6;
      border-top-left-radius: 6px;
      border-bottom-left-radius: 6px;}
    table.table.list-table tbody > tr > td:last-child,
    table.table.list-table tbody > tr > td.cluster-last {
      border-right: 1px solid #dee2e6;
      border-top-right-radius: 6px;
      border-bottom-right-radius: 6px;}
    table.table.list-table tbody > tr > td a {
      color: #808080;
      text-decoration: none;}

  table.table.list-table thead td,
  table.table.list-table thead th {
    color: #808080;
    background-color: #f2f2f2;
    height: 100%;
    padding: 10px 0 0;
    border-top: none;
    font-size: .5rem;
    vertical-align: bottom;
    text-align: center;}
    table.table.list-table thead > tr > td.cluster-first div {
      border-top-left-radius: 6px;}
    table.table.list-table thead > tr > td.cluster-last div {
      border-top-right-radius: 6px;}
    table.table.list-table thead > tr > td:first-child {
      border-left: none;}
    /* テーブルヘッダー下辺の色付き線設定 */
    table.table.list-table thead > tr > td.bottom-gray {
      border-bottom: 2px solid #cccccc;}
    table.table.list-table thead > tr > td.bottom-dark-green {
      border-bottom: 2px solid #abc6c5;}
    table.table.list-table thead > tr > td.bottom-yellow-green {
      border-bottom: 2px solid #cad5c7;}
    table.table.list-table thead > tr > td.bottom-yellow {
      border-bottom: 2px solid #e6e0c0;}
    table.table.list-table thead > tr > td.bottom-orange {
      border-bottom: 2px solid #e0caaf;}
      table.table.list-table thead td > div,
      table.table.list-table thead th > div {
        position: relative;
        padding: 5px 0;
        background-color: #e6e6e6;
        height: 5.5em;}
        table.table.list-table thead td > div > span,
        table.table.list-table thead th > div > span {
          position: absolute;
          bottom: .25em;
          width: 100%;
          left: 0;}
        table.table.list-table thead td > div > .head-border,
        table.table.list-table thead th > div > .head-border {
          position: absolute;
          bottom: 0;
          border-left: 1px solid #f2f2f2;
          height: 5em;}

  /* テーブルの表の中身の設定 */
  table.table.list-table > tbody .icon,
  table.table.list-table > tbody .datetime,
  table.table.list-table > tbody .check {
    border-left: 1px solid #dee2e6;
    text-align: center;
    margin: 0 -.75rem;
    min-height: 50px;}
    table.table.list-table > tbody .cluster-first .icon,
    table.table.list-table > tbody .cluster-first .datetime,
    table.table.list-table > tbody .cluster-first .check {
      border-left: 0;}
  table.table.list-table > tbody .icon i {
    line-height: 1.5;
    font-size: 2rem;}
  table.table.list-table > tbody .last-day {
    font-size: .8rem;
    text-align: center;
    margin: 0 -.75rem;}
  table.table.list-table > tbody .datetime,
  table.table.list-table > tbody .check {
    font-size: .8rem;
    padding-top: .3rem;}
    .table.table.list-table.list-table-target > tbody .datetime{
      padding-top: 0;}
  table.table.list-table > tbody .datetime-col .datetime {
    padding-top: 1rem;}
    table.table.list-table > tbody .datetime-col:first-child .datetime {
      border-left: 0;}
  
  /* テーブルの中のバツ以外のアイコンの色を薄くする */
  table.table.list-table .icon-success,
  table.table.list-table .icon-default {
    opacity: .5;}

  /* 抽出ページのテーブルの設定 */
  table.list-table-target tbody tr > th > div,
  table.list-table-target tbody tr > td > div {
    font-size: .8rem;
    border-left: 1px solid #dee2e6;
    padding: 0 .75rem;
    margin: 0 -.75rem;
    height: 100%;
    letter-spacing: 1.6px;
    line-height: 1.75;
    text-align: justify;}
    table.list-table-target tbody tr > th > .datetime,
    table.list-table-target tbody tr > td > .datetime {
      padding-top: 0;}
    table.list-table-target tbody tr > th:first-child > div,
    table.list-table-target tbody tr > td:first-child > div {
      border-left: 0;}
    table.list-table-target tbody tr > th.image > div,
    table.list-table-target tbody tr > td.image > div {
      text-align: center;}

  /* 各ページのテーブルのレイアウト設定 */
  table.main-list-table tr > th,
  table.main-list-table tr > td {
    width: 5.5555555555%;}
    table.main-list-table tr > th.url,
    table.main-list-table tr > td.url {
      width: 27.777777777%;}
    table.main-list-table tr > th.offset,
    table.main-list-table tr > td.offset {
      width: 8.3333333332%;}
    table.main-list-table.main-list-table-admin tr > th.url,
    table.main-list-table.main-list-table-admin tr > td.url {
      width: 16.666666666%;}

  table.list-table-site-detail tr > th,
  table.list-table-site-detail tr > td {
    width: 5.5555555555%;}
    table.list-table-site-detail > thead > tr > th.datetime-col.first-col,
    table.list-table-site-detail > thead > tr > td.datetime-col.first-col {
      width: 16.666666666%;}
    table.list-table-site-detail > thead > tr > th.datetime-col,
    table.list-table-site-detail > thead > tr > td.datetime-col {
      width: 11.111111111%;}
    table.list-table-site-detail > thead > tr > th.offset,
    table.list-table-site-detail > thead > tr > td.offset {
      width: 13.888888888%;}
  table.list-table-target tr > th,
  table.list-table-target tr > td {
    width: 33.333333%;}
    table.list-table-target > thead > tr > th.result-col,
    table.list-table-target > thead > tr > td.result-col {
      width: 22.222222%;}
    table.list-table-target > thead > tr > th.datetime-col,
    table.list-table-target > thead > tr > td.datetime-col {
      width: 11.111111%;}
